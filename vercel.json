{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["codigo/assets/javascript/Json/login.json"]
      }
    },
    {
      "src": "codigo/html/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "codigo/assets/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/codigo/assets/$1"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/codigo/html/$1"
    },
    {
       "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
